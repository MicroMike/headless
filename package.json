{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "50%": "cross-env FILE=spotifyAccount.txt node favS",
    "startn": "concurrently \"npm run startp\" \"npm run startn\"",
    "start": "cross-env FILE=spotifyAccount.txt ALLNEW=true node favS",
