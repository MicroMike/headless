    "s": "git reset --hard && git pull && chmod -R 777 . && npm run start"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^2.0.0"
  },
  "dependencies": {
    "ajax-request": "^1.2.3",
    "concurrently": "^4.0.1",
    "electron-widevinecdm": "^7.0.0",
    "nightmare": "^3.0.1"
  }
}
